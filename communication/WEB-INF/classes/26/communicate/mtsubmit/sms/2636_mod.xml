<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

	<bean id="mtsGatewayModule2636" class="com.lxt2.gateway.sms.module.SmsGatewayModule">
		<property name="id" value="2636"/>
		<property name="name" value="下行发送CMPP20"/>
		<property name="hub" ref="hubImpl"/>
		<property name="submitQueue" ref="mtsSubmitQueue2636"/>
		<property name="adapter" ref="mtsPersistentSubmitQueueAdapter2636"/>
		<property name="gateway" ref="mtsCmpp20Gateway2636"/>
		<!--property name="status" value="STOP"/-->
		<property name="configList">
			<list>
				<ref bean="mtsCmpp20Parameter2636"/>
				<ref bean="mtsCmpp20DeliverAdapter2636"/>
			</list>
		</property>
		<!-- 命令集合 -->
		<property name="controlCmdMap">
			<map>
				<entry key="qePerf" value-ref="mtsQueueCmd2636"/>
                <entry key="view" value-ref="mtsGateWayViewCmd2636"/>
			</map>
		</property>
		<!-- 指标集合 -->
		<property name="kpiList">
			<list>
				<ref bean="mtsQueueKPI2636"/>
				<ref bean="mtsGateWayModuleKPI2636"/>
			</list>
		</property>
	</bean>

	<!-- 模块的指标配置 -->
	<bean id="mtsQueueKPI2636" class="com.lxt2.queuekc.QueueKPI">
		<property name="queue" ref="mtsSubmitQueue2636"/>
	</bean>
	<!-- 模块的命令配置 -->
	<bean id="mtsQueueCmd2636" class="com.lxt2.queuekc.QueuePerfCmd">
		<property name="kpiQueueList">
			<list>
				<ref bean="mtsQueueKPI2636"/>
			</list>
		</property>
	</bean>

	<bean id="mtsSubmitQueue2636" class="com.lxt2.queuekc.CacheQueue" destroy-method="destroy">
		<property name="cache">
			<bean class="ihandy.lxt20.persist.cache.FCCache"/>
		</property>
		<property name="queueName" value="mtsSubmit2636"/>
	</bean>
	
	<bean id="mtsPersistentSubmitQueueAdapter2636" class="com.lxt2.gateway.sms.queue.PersistentSubmitQueueAdapter">
		<property name="cacheQueue" ref="mtsSubmitQueue2636"/>
		<property name="hub" ref="hubImpl"/>
		<property name="spliterMap" ref="mtsSpliterMap2636"/>
		<property name="config" ref="mtsCmpp20GatewayConfig2636"/>
	</bean>
	<bean id="mtsPersistentRespQueueAdapter2636" class="com.lxt2.gateway.sms.queue.PersistentRespQueueAdapter">
		<property name="hub" ref="hubImpl"/>
		<property name="sid" ref="ndSid"/>
		<property name="config" ref="mtsCmpp20GatewayConfig2636"/>
        <property name="saveLogger" ref="saveLogger" />
<!-- 		<property name="proc" value="1004"/> -->
	</bean>
	<bean id="mtsPersistentReportQueueAdapter2636" class="com.lxt2.gateway.sms.queue.PersistentReportQueueAdapter">
		<property name="hub" ref="hubImpl"/>
		<property name="reportAdapter" ref="mtsCmpp20reportAdapter2636"/>
<!-- 		<property name="proc" value="1004"/> -->
		<property name="sid" ref="ndSid"/>
		<property name="config" ref="mtsCmpp20GatewayConfig2636"/>
	</bean>
	<bean id="mtsPersistentDeliverQueueAdapter2636" class="com.lxt2.gateway.sms.queue.PerisitentDeliverQueueAdapter">
		<property name="hub" ref="hubImpl" />
		<property name="deliverAdapter" ref="mtsCmpp20DeliverAdapter2636"/>
		<property name="sid" ref="ndSid"/>
		<property name="config" ref="mtsCmpp20GatewayConfig2636"/>
	</bean>
    <bean id="mtsGateWayModuleKPI2636" class="com.lxt2.gateway.sms.module.SmsGatewayModuleKPI">
        <property name="cacheQueue" ref="mtsSubmitQueue2636"/>
        <property name="dataGather" ref="mtsDataGather2636"/>
        <property name="module" ref="mtsGatewayModule2636"/>
    </bean>
    <bean id="mtsDataGather2636" class="com.lxt2.gateway.sms.GatewayDataGather" init-method="init" destroy-method="destroy">
		<property name="path" value="${CLASSPATH}/${nodeID}/persistent/statistics/gateway2636.properties"/>
    </bean>
    <!-- 获取模块的监视信息 -->
	<bean id="mtsGateWayViewCmd2636" class="com.lxt2.base.module.ModuleKPICmd">
		<property name="kpi" ref="mtsGateWayModuleKPI2636"/>
	</bean>
	<!-- 拆分抽象bean -->
	<bean id="mtsAbstractSpliter2636" abstract="true" lazy-init="true">
		<property name="nodeGatewayID" value="${nodeID}" />
		<property name="gatewayId" ref="gatewayId2636"></property>
		<property name="gatewayXmlParser" ref="gatewayXml"></property>
	</bean>
	<bean id="mtsSpliterMap2636" class="java.util.HashMap" scope="prototype">
		<constructor-arg index="0">
			<map>
				<entry key="15">
					<!-- 普通短信拆分类 -->
					<bean class="com.techcenter.spliter.CommSmsSpliter" parent="mtsAbstractSpliter2636">
						<property name="smSplit">
							<bean class="com.techcenter.spliter.SMSplit" />
						</property>
					</bean>
				</entry>
				<entry key="31">
					<!-- wappush拆分类 -->
					<bean class="com.techcenter.spliter.WapPushSpliter" />
				</entry>
				<entry key="32">
					<!-- 长短信拆分类 -->
					<bean class="com.techcenter.spliter.LongSmsSpliter" parent="mtsAbstractSpliter2636" />
				</entry>
			</map>
		</constructor-arg>
	</bean>
</beans>
